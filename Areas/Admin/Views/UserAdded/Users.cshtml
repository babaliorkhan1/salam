@model List<AppUser>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>User List</title>
    <style>
        body {
            font-family: Arial, sans-serif;
        }

        .user-list {
            width: 30%;
            height: 100px;
            overflow-y: scroll;
            border: 1px solid #ccc;
            padding: 10px;
            margin-left: 550px;
        }

        .user-item {
            padding: 10px;
            border-bottom: 1px solid #eee;
        }
    </style>
</head>
<body>
    <h1>User List</h1>
    <div class="user-list" style="height:300px;">
        @foreach (var item in Model)
        {
            <div class="user-item">
                <a asp-area="" asp-controller="QrKod" asp-action="CreateQrCode" asp-route-id="@item.Id"><strong>@item.UserName</strong></a>
                <a asp-area="" asp-controller="QrKod" asp-action="CreateQrCode" asp-route-id="@item.Id"><strong>Create Qr Code for this username</strong></a>
               
            </div>
        }


    </div>
   
</body>
</html>
